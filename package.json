{
  "name": "synth",
  "version": "0.0.1",
  "description": "a javascript synth",
  "dependencies": {
    "express": "^4.13.3",
    "debug": "~2.2.0",
    "jade": "~1.11.0"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-jshint": "^2.0.0",
    "gulp-sass": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "jshint": "^2.9.1-rc2"
  },
  "scripts": {
    "start": "node ./bin/www"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamwdraper/synth.git"
  },
  "jshintConfig": {
    "node": true,
    "browser": true,
    "curly": true,
    "devel": true,
    "eqeqeq": true,
    "noarg": true,
    "sub": true,
    "expr": true,
    "globals": {
      "define": false,
      "describe": false,
      "it": false,
      "assert": false,
      "expect": false,
      "require": false,
      "requirejs": false
    },
    "strict": false
  },
